@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}



<table class="tb" style="width:800px;">
    <thead>
        <br>
        <tr>
      
            <th>
                Username
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
            
                <td>
                    @item.UserName
                </td>
                <td>
                    @item.Email
                </td>
            </tr>
        }
    </tbody>
</table>
